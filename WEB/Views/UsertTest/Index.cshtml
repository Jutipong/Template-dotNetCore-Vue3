@using System.Text.Json;
@model WEB.Models.UserTest.UserTestViewModel
@{ ViewData["Title"] = "Demo01 - Vue3"; }

<app-component></app-component>

@section scripts{

    <environment names="Development">
        <script src="~/js/UserTest/datatable-setup.js" asp-append-version="true"></script>
        <script src="~/js/UserTest/store.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/js/UserTest/datatable-setup.min.js" asp-append-version="true"></script>
        <script src="~/js/UserTest/store.min.js" asp-append-version="true"></script>
    </environment>

    @*---------- optional component ----------*@
    @await Html.PartialAsync("~/Views/_VueComponent/v-datepicker.cshtml")
    @await Html.PartialAsync("~/Views/_VueComponent/v-select2.cshtml")
    @await Html.PartialAsync("~/Views/_VueComponent/v-number.cshtml")
    @await Html.PartialAsync("~/Views/_VueComponent/v-table.cshtml")
    @*-------------- component -------------*@
    @await Html.PartialAsync("~/Views/UsertTest/Components/App.cshtml")

    <script type="text/javascript">
    //=====================================
    //====== STATE GLOBAL (◍•ᴗ•◍)❤ =======
    //=====================================
        _initialModel(@Html.Raw(JsonSerializer.Serialize(Model.UserTest)), @Html.Raw(JsonSerializer.Serialize(Model.UserTest)));
    </script>
}